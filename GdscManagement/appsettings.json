{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Authentication":{
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "Some audiences here"
        ],
        "ValidIssuers": [
          "Some issuers here"
        ],
        "SigningKeys": [
          {
            "Issuer": "Same for each valid issuer",
            "Value": "Some base 64 string"
          }
        ]
      }
    }
  },
  "AllowedHosts": "*"
}
